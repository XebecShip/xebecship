{
  "name": "@xebecship/ui",
  "version": "0.0.11",
  "description": "Xebec UI - A React component library for building modern web applications.",
  "type": "module",
  "author": "XebecShip",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "src/theme.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./theme.css": "./src/theme.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "check": "tsc --noEmit",
    "build": "vite build",
    "release": "pnpm build && pnpm version patch && pnpm publish --access public"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@fontsource-variable/space-grotesk": "^5.2.8",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@xebecship/config": "workspace:*",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^6.0.3"
  }
}
